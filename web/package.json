{
  "name": "svelte-tailwind-snowpack",
  "version": "0.1.0",
  "author": "Agney Menon <agney@outlook.in>",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "keywords": [
    "csa-template"
